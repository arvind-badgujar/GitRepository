<html lang='en'>
<head>
    <title>Git Repository</title>
    <meta charset='utf-8'>
    <meta name='viewport' content='width=device-width, initial-scale=1'>
    <link rel='stylesheet' href='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css'>
    <script src='https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js'></script>
    <script src='https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js'></script>
</head>
<body>

<div class='container'>
    <table class='table table-striped table-hover table-bordered'>
        <thead>
        <tr>
            <th class="text-center" colspan="5">Repository Table</th>
        </tr>
        <tr>
            <th class="text-center">Repository Id</th>
            <th class="text-center">Repository Name</th>
            <th class="text-center">Full Name</th>
            <th class="text-center">Url</th>
            <th class="text-center">Description</th>
        </tr>
        </thead>
        <tbody>
            {% for repositoryDetail in repositoryDetails %}
                {% for repository in repositoryDetail %}
                    <tr>
                        <td>{{ repository['Id'] }}</td>
                        <td>{{ repository['Name'] }}</td>
                        <td>{{ repository['FullName'] }}</td>
                        <td>
                            <a href="{{ repository['Url'] }}" target="_blank">{{ repository['Url'] }}</a>
                        </td>
                        <td>{{ repository['Description'] }}</td>
                    </tr>
                {% endfor %}
            {% endfor %}
        </tbody>
    </table>
</div>

</body>
</html>